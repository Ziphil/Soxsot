    "eslint-plugin-jsdoc": "^30.7.13",
    "jest": "^26.6.3",
    "mock-fs": "^4.14.0",
    "ts-dedent": "^2.1.1",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
