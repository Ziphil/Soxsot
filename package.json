{
  "name": "soxsot",
  "version": "0.8.1",
  "author": "Ziphil",
  "description": "Library for manipulating Shaleian dictionaries",
  "homepage": "https://github.com/Ziphil/Soxsot",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "develop": "ts-node source/index.ts",
    "build": "tsc --project tsconfig-build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint source/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix source/**/*.ts test/**/*.ts",
    "prepare": "tsc --project tsconfig-build.json"
  },
  "eslintConfig": {
    "extends": "ziphil"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.(ts|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "ts-jest",
        {
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "verbose": true
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/mock-fs": "^4.13.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint": "^8.17.0",
    "eslint-config-ziphil": "^1.6.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^29.6.4",
    "jest-extended": "^0.11.5",
    "mock-fs": "^5.2.0",
    "ts-dedent": "^2.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "talqis": "^0.1.0",
    "ts-essentials": "^7.0.2",
    "uuid": "^8.3.2"
  }
}
